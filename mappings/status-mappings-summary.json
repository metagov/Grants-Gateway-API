{
  "title": "Comprehensive Status Mappings for All Grant Systems",
  "lastUpdated": "2025-09-22",
  "description": "Consolidated view of how each grant system's status values map to DAOIP-5 standards and how they're handled in frontend charts",
  "chart_logic": {
    "funding_calculation_includes": [
      "funded",
      "approved", 
      "awarded",
      "completed",
      "submitted"
    ],
    "badge_styling": {
      "default_variant": ["funded", "awarded", "completed", "submitted"],
      "secondary_variant": ["approved"],
      "destructive_variant": ["rejected"],
      "outline_variant": ["pending", "unknown"]
    }
  },
  "systems": {
    "octant": {
      "adapter_type": "custom",
      "native_logic": "Based on allocation amount",
      "mappings": {
        "allocation > 0": {
          "daoip5_status": "funded",
          "chart_treatment": "counted_as_funded",
          "badge_variant": "default"
        },
        "allocation = 0": {
          "daoip5_status": "pending", 
          "chart_treatment": "not_counted",
          "badge_variant": "outline"
        }
      }
    },
    "giveth": {
      "adapter_type": "custom",
      "native_logic": "Based on QF round activity",
      "mappings": {
        "qfRound.isActive = true": {
          "daoip5_status": "approved",
          "chart_treatment": "counted_as_funded",
          "badge_variant": "secondary"
        },
        "qfRound.isActive = false": {
          "daoip5_status": "completed",
          "chart_treatment": "counted_as_funded", 
          "badge_variant": "default"
        }
      }
    },
    "celo": {
      "adapter_type": "daoip5_static",
      "native_logic": "Uses DAOIP-5 compliant status values",
      "special_case": "Uses 'submitted' status which is Celo-specific",
      "mappings": {
        "submitted": {
          "daoip5_status": "submitted",
          "chart_treatment": "counted_as_funded",
          "badge_variant": "default",
          "note": "Celo-specific status, treated as funded in calculations"
        },
        "pending": {
          "daoip5_status": "pending",
          "chart_treatment": "not_counted",
          "badge_variant": "outline"
        },
        "approved": {
          "daoip5_status": "approved", 
          "chart_treatment": "counted_as_funded",
          "badge_variant": "secondary"
        },
        "completed": {
          "daoip5_status": "completed",
          "chart_treatment": "counted_as_funded",
          "badge_variant": "default"
        },
        "rejected": {
          "daoip5_status": "rejected",
          "chart_treatment": "not_counted", 
          "badge_variant": "destructive"
        }
      }
    },
    "stellar": {
